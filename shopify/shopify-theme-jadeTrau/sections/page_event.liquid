<style>
.clearfix{clear:both:}
.container{max-width:1440px;margin:0 auto}
.background_bg{background-color:#3F3E40}
.max_stay{margin:0 auto;max-width:686px;text-align:center}
.max_stay h1{margin:0;padding:65px 0 0;font-size:42px!important;color:#FFF;text-transform:uppercase;letter-spacing:0;font-family:'GoldenbookW01'!important;font-weight:700!important;text-align:center}
.max_stay p{padding:22px 0 20px;font-size:16px!important;color:#FFF;line-height:24px;text-align:center;font-family:sofia-pro!important;font-weight:400!important;max-width:586px;margin:0 auto}
.max_stay .btn{background-color:transparent;border:1px solid #FFF;color:#FFF;border-radius:0;font-size:16px!important;text-transform:uppercase;width:47%;padding:0;margin:0 0 75px;font-family:'Sofia Pro'!important;font-weight:500!important;height:50px;display:inline-flex;justify-content:center;align-items:center}
.gallery_img{position:relative;cursor:pointer;transition:all .3s ease-in-out;margin:0 0 30px}
.gallery_title{position:absolute;bottom:0;opacity:0;transition:all .3s ease-in-out}
.gallery_title h4{margin:0;padding:4px 10px;font-size:12px!important;color:#FFF;font-family:'Sofia Pro'!important;font-weight:400!important;background-color:#000;display:table;transition:all .3s ease-in-out}
.gallery_img:hover .gallery_title{opacity:1;transition:all .3s ease-in-out}
.gallery_event h3{margin:0;padding:0 15px 44px;font-size:36px!important;text-transform:uppercase;font-weight:2!important;color:#FFF;letter-spacing:1px}
.loard_more h4{margin:0;padding:25px 0 48px;color:#FFF;font-size:16px!important;font-family:'Sofia Pro'!important;font-weight:500!important;text-align:center}
.blog_event_main{border:1px solid #E5E5E5;padding:0 0 25px;width:95%;margin:0 auto 88px}
.black_border h4{
  margin:0;
  padding:15px 0 0 0px;
  width:100%;
  display:inline-flex;
  align-items:center;
  font-weight:500!important;
  text-transform:uppercase;
  font-size: 24px !important;
  line-height: 30px;
  color: #FFF;
  letter-spacing: 0px;
}
.black_border h4 a{color:#FFF}
.width_max{width:100%}
.blog_date_main{display:flex;justify-content:space-between;align-items:center;width:100%}
.blog_part{padding:0px 30px;height:363px}
.blog_date h3{margin:0;padding:0;font-size:16px!important;color:#FFF;font-family:'Sofia Pro'!important;font-weight:400!important;text-transform:uppercase}
.blog_time h4{margin:0;padding:0;font-size:16px!important;color:#FFF;font-family:'Sofia Pro'!important;font-weight:300!important;text-align:end}
.blog_part p{margin:0;padding:25px 0 0;font-size:14px!important;color:#FFF;font-family:'Sofia Pro'!important;font-weight:300!important;line-height:22px;height:170px}
.blog_part h5{margin:0;padding:0px 0 0 0;font-size:16px!important;font-family:'Sofia Pro';font-weight:400;color:#FFF;font-family:'Sofia Pro'!important;font-weight:500!important}
.blog_padding{padding:40px 0 0}
.add_calender .btn{margin:0;padding:0;font-size:16px!important;text-transform:capitalize;background-color:transparent!important;font-family:'Sofia Pro'!important;font-weight:500!important;color:#FFF;border:none}
.add_book .btn{margin:0;padding:0;font-size:16px!important;color:#3F3E40;font-family:'Sofia Pro';font-weight:500;background-color:#FFF;width:100%;height:47px;float:right;display:inline-flex;justify-content:center;align-items:center;text-transform:capitalize;font-family:'Sofia Pro'!important;font-weight:500!important}
.blog_event .slick-prev{left:-4%;text-align:center;right:0;padding:30px;top:50%;transform:translate(0,-50%);z-index:999;opacity:1;background-image:url({{ 'left.png' | asset_url }});background-repeat:no-repeat;background-position:center;outline:none}
.blog_event .slick-next{text-align:center;right:-4%;padding:30px;top:50%;transform:translate(0,-50%);opacity:1;background-image:url({{ 'right.png' | asset_url }});background-repeat:no-repeat;background-position:center;outline:none}
.blog_event .slick-next:before{content:'';font-family:"Font Awesome 5 Brands"}
.blog_event .slick-prev:before{content:'';font-family:"Font Awesome 5 Brands";font-family:"Font Awesome 5 Brands"}
.blog_event .slick-next:before{content:'';font-family:"Font Awesome 5 Brands"}
.blog_event .slick-prev:hover,.blog_event .slick-prev:focus{background-image:url({{ 'left.png' | asset_url }});background-repeat:no-repeat;background-position:center}
.blog_event .slick-next:hover,.blog_event .slick-next:focus{background-image:url({{ 'right.png' | asset_url }});background-repeat:no-repeat;background-position:center}
.blog_event .slick-slider{margin-bottom:0}
.gallery_event{width:100%}
.loard_more{width:100%}
.max_stay .btn:hover{background-color:#00000078}
.col_lg{width:33%;padding:0 15px}
.max_slider{width:1400px}
.add_book .btn:hover{background-color:#00000047;color:#fff}
