<div class="background_bg">
  <section class="page_event">
    <div class="banner_img">
      {% if section.settings.image != blank %}
      {%- assign img_url = section.settings.image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
      <img class="lazyload lazypreload"
           src="{{ section.settings.image | img_url: '300x300' }}"
           data-src="{{ img_url }}"
           data-widths="[180, 360, 470, 600, 770, 970, 1060, 1280, 1512, 1728, 2048]"
           data-aspectratio="{{ section.settings.homebanner.aspect_ratio }}"
           data-sizes="auto"
           alt="{{ section.settings.image.alt | escape }}" width="100%"> 
      {% else %}
      <div class="onboard">
        {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
      </div>
      {% endif %}
    </div>
  </section>
  <section class="page_stay">
    <div class="container">
      <div class="max_stay">
        {% if section.settings.title != blank %}
        <h1>{{ section.settings.title }}</h1>
        {% endif %}
        {% if section.settings.tag_line != blank %}
        <p>{{ section.settings.tag_line }}</p>
        {% endif %}
        {% if section.settings.link != blank %}
        <a href="{{ section.settings.link }}" class="btn">{{ section.settings.btn }}</a>
        {% endif %}
      </div>

    </div>
  </section>
  <section class="page_upcomming">
    <div class="container">
      <div class="row">
        <div class="gallery_event">
          {% if section.settings.gallery_event1 != blank %}
          <h3>{{ section.settings.gallery_event1 }}</h3>
          {% endif %}
        </div>
        <div class="max_slider">
          <div class="blog_event">
            {% for block in section.blocks %}
            {% if block.type == 'blog' %}
            <div>
              <div class="slider">
                <div class="">
                  <div class="blog_event_main">
                    {% if block.settings.image2 != blank %}
                    {%- assign img_url = block.settings.image2 | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
                    <img class="lazyload lazypreload"
                         src="{{ block.settings.image2 | img_url: '300x300' }}"
                         data-src="{{ img_url }}"
                         data-widths="[180, 360, 470, 600, 770, 970, 1060, 1280, 1512, 1728, 2048]"
                         data-aspectratio="{{ block.settings.homebanner.aspect_ratio }}"
                         data-sizes="auto"
                         alt="{{ block.settings.image2.alt | escape }}" width="100%"> 
                    {% else %}
                    <div class="onboard">
                      {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                    </div>
                    {% endif %}
                   
                    <div class="blog_part">
                      <div class="blog_date_main">
                        <div class="blog_date width_max">
                          {% if block.settings.date != blank %}
                          <h3>{{ block.settings.date }}</h3>
                          {% endif %}
                        </div>
                        <div class="blog_time width_max">
                          {% if block.settings.time != blank %}
                          <h4>{{ block.settings.time }}</h4>
                          {% endif %}
                        </div>
                      </div>
                       <div class="black_border">
                      {% if block.settings.black_border != blank %}
                      <h4><a href="{{ block.settings.blog_link }}">{{ block.settings.black_border }}</a></h4>
                      {% endif %}  
                    </div>
                      {% if block.settings.decs != blank %}
                      <p>{{ block.settings.decs }}</p>
                      {% endif %}
                      {% if block.settings.job != blank %}
                      <h5 style="text-transform:none !important;">{{ block.settings.job }}</h5>
                      {% endif %}
                      <div class="blog_date_main blog_padding">
                       <!-- <div class="width_max add_calender">
                          {% if block.settings.add_calender_link != blank %}
                          <a href="{{ block.settings.add_calender_link }}" class="btn">{{ block.settings.add_calender }}</a>
                          {% endif %}
                        </div>-->
                        <div class="width_max add_book">
                          {% if block.settings.add_book != blank %}                        
                            <a target="_blank" href="{{ block.settings.add_book_link }}"  class="btn">{{ block.settings.add_book }}</a>
                          {% endif %}
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            {% endif %}
            {% endfor %}
          </div>
        </div>
      </div>
    </div>
  </section>
  <section class="page_gallery">
    <div class="container">
      <div class="row">
        <div class="gallery_event">
          {% if section.settings.gallery_event != blank %}
          <h3>{{ section.settings.gallery_event }}</h3>
          {% endif %}
          <div class="clearfix"></div>
        </div>

        {% for block in section.blocks %}
        {% if block.type == 'gallery' %}
        <div class="col_lg">
          <div class="gallery_img">
            {% if block.settings.image1 != blank %}
            {%- assign img_url = block.settings.image1 | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
            <img class="lazyload lazypreload"
                 src="{{ block.settings.image1 | img_url: '300x300' }}"
                 data-src="{{ img_url }}"
                 data-widths="[180, 360, 470, 600, 770, 970, 1060, 1280, 1512, 1728, 2048]"
                 data-aspectratio="{{ block.settings.homebanner.aspect_ratio }}"
                 data-sizes="auto"
                 alt="{{ block.settings.image1.alt | escape }}" width="100%"> 
            {% else %}
            <div class="onboard">
              {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
            </div>
            {% endif %}
            <div class="gallery_title">
              {% if block.settings.gallery_title != blank %}
              <h4>{{ block.settings.gallery_title }}</h4>
              {% endif %}
            </div>
          </div>
        </div>
        {% endif %}
        {% endfor %}
        <div class="clearfix"></div>
       <!-- <div class="loard_more">
          {% if section.settings.load_more != blank %}
          <h4>{{ section.settings.load_more }}</h4>
          {% endif %}
        </div>-->

      </div>
    </div>
  </section>
</div>

