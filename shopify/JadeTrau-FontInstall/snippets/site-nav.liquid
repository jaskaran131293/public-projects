<!--site-nav.liquid--------->
<!--Top Menu -->

<div class=" megamenu">
  <ul class="navbar-nav site-nav new-megamenu">
    {% assign menu = linklists[section.settings.menu] %}
    {% for link in menu.links %}
    {% capture childrens %}
    {% assign megamenu = null %}
    {% for block in section.blocks %}
    {% if block.type == 'megamenu' %}
    {% assign for_item_handle = block.settings.for_item | handle %}
    {% if for_item_handle == link.handle %}
    {% assign megamenu = block.settings %}
     {% if for_item_handle == 'shop' or for_item_handle == 'discover' %}
    {% include 'megamenu_1' %}
     
      {% else %}
     {% include 'megamenu_1_alternate' %}
      {% endif %}
    {% endif %}
    {% endif %}
    {% endfor %}
    {% if megamenu == null %}
    <div class="dropdown-sub level1">
      <ul>
        {% for sub_link in link.links %}
        <li class="{% if sub_link.levels > 0 %} menu-dropdown {%endif %}">
          {% include 'menu-labels' with link: link sub_link: sub_link %}
          <a href="{{ sub_link.url }}" class="">
            <span>{{ sub_link.title }}</span>
            {{ labels }}{% if sub_link.levels > 0 %} <i class="cs-font clever-icon-arrow-right"></i>{% endif %}</a>
          {% if sub_link.levels > 0 %}
          <div class="dropdown-sub level2">
            <ul>
              {% for sub_sub_link in sub_link.links %}
              {% include 'menu-labels' with link: link sub_link: sub_link sub_sub_link: sub_sub_link %}
              <li class="menu__item">
                <a href="{{ sub_sub_link.url }}" class="">
                <span>{{ sub_sub_link.title }}</span>
                {{ labels }}</a>
              </li>
              {% endfor %}
            </ul>
          </div>
          {% endif %}
        </li>
        {% endfor %}
      </ul>
    </div>
    {% endif %}
    {% endcapture %}
     {% assign shoptitle = link.title | downcase %}
      
    <li class="  level-top {% if link.levels > 0 %}menu-dropdown{% if megamenu != null %} megamenu-dropdow{% endif %}{% endif %} {{ link.title | handleize }}" >
      {% include 'menu-labels' with link: link %}
      <a href="{{ link.url }}" class="" onmouseover="openhovertabshop('{{ link.title | downcase  }}')" onmouseout="openhovertabshop12('{{ link.title | downcase  }}')" >
        <span>{{ link.title }}</span>
        {{ labels }}{% if link.levels > 0 %}<i class="cs-font clever-icon-down"></i>{% endif %}</a>
      {% if link.levels > 0 or megamenu != null %}
      {{ childrens }}
      {% endif %}
    </li>
    {% endfor %}
  </ul>
</div>  

<!-- End Menu -->
