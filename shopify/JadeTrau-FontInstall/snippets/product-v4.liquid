                    {% assign product_variants = product_variants | append: ',' | append: v.id %}
                  {% endif %}
                {% endfor %}
                

                {% assign class = '' %}
                {% if image.id == mt_current_variant.featured_media.id and addActiveClass %}
                  {% assign class = 'active-magic-selector' %}
                {% assign addActiveClass = false %}
                {% endif %}        
